# Google OAuth credentials
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_GOOGLE_CLIENT_SECRET=your-google-client-secret

# Spotify API credentials
VITE_SPOTIFY_CLIENT_ID=your-spotify-client-id
VITE_SPOTIFY_CLIENT_SECRET=your-spotify-client-secret

# MongoDB connection
# החלף את זה במחרוזת החיבור האמיתית שלך מ-MongoDB Atlas
VITE_MONGODB_URI=mongodb+srv://username:password@cluster0.xxxxx.mongodb.net/fretone?retryWrites=true&w=majority

# Apple Music API credentials
VITE_APPLE_MUSIC_TEAM_ID=your-apple-music-team-id
VITE_APPLE_MUSIC_KEY_ID=your-apple-music-key-id
